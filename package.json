{
  "name": "ae-rpc",
  "version": "1.0.0",
  "description": "Discord Rich Presence for AE made by Kureichi",
  "scripts": {
    "copy": "shx mkdir -p dist/ && shx cp -r CSXS/ dist/CSXS && shx cp -r jsx/ dist/ && shx cp .debug dist/.debug",
    "build:extension": "parcel build ./extension/index.html --dist-dir ./dist/extension --no-source-maps --public-url ./",
    "build:panel": "parcel build ./panel/index.html --dist-dir ./dist/panel --no-source-maps --public-url ./",

    "build": "pnpm run copy && pnpm run build:extension && pnpm run build:panel"
  },
  "author": "Kureichi",
  "contributors": [
    "mar <https://github.com/omaralhami>"
  ],
  "license": "ISC",
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "discord-rpc": "^4.0.1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.16.0",
    "@parcel/core": "^2.16.0",
    "@parcel/plugin": "^2.16.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "parcel": "^2.16.0",
    "process": "^0.11.10",
    "shx": "^0.4.0",
    "timers-browserify": "^2.0.12"
  }
}
